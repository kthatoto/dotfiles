{
  "typescript.suggest.autoImports": true,
  "javascript.suggest.autoImports": true,
  "languageserver": {
    "sorbet": {
      "command": "srb",
      "args": ["tc", "--lsp"],
      "filetypes": ["ruby"],
      "rootPatterns": ["sorbet/config", "Gemfile", ".git"]
    },
    "typespec": {
      "command": "tsp-server",
      "args": ["--stdio"],
      "filetypes": ["typespec"],
      "rootPatterns": ["tspconfig.yaml", ".git"]
    }
  },
  "coc.preferences.formatOnSave": true,
  "prettier.bracketSpacing": true,
  "prettier.printWidth": 110
}
